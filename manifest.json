{
  "manifest_version": 3,
  "name": "LigaMagic CardBlocker",
  "version": "1.0",
  "permissions": ["storage", "downloads", "unlimitedStorage"],
  "content_scripts": [
    {
      "js": [
        "remover.js"
      ],
      "matches": [
        "https://*/*"
      ]
    },
    {
      "js": [
        "blockButton.js"
      ],
      "matches": [
        "https://*/*"
      ]
    },
    {
      "js": [
        "buttonHandler.js"
      ],
      "matches": [
        "https://*/*"
      ]
    },
    {
      "js": [
        "init.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}